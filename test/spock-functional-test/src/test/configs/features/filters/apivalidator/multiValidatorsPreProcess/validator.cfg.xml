<?xml version="1.0" encoding="UTF-8"?>

<validators multi-role-match="true" xmlns='http://openrepose.org/repose/validator/v1.0'>
   <validator
      enable-pre-process-extension="true"
      role="admin"
      default="false"
      wadl="other.wadl"
      check-well-formed="false"
      check-elements="true"
      xsl-engine="XalanC"
      enable-rax-roles="true"
      dot-output="/tmp/default.dot"
      use-saxon="false"/>

   <validator
      role="default"
      enable-rax-roles="true"
      default="true"
      wadl="default.wadl"
      check-well-formed="false"
      check-elements="true"
      xsl-engine="XalanC"
      enable-pre-process-extension="true"
      check-plain-params="true"
      dot-output="/tmp/other.dot"
      use-saxon="false"/>
</validators>

